// O método tirazeros elimina todos os elementos nulos
// de v[0..n-1]. Ele supe que n >= 0.
// O método deixa o resultado em v[0..i-1] e devolve i.

int tirazeros (int n, int v[])
{
   int i, j;
   i = 0;
   for (j = 0; j < n; ++j)
      if (v[j] != 0)
         {
          v[i] = v[j];
          i++;
         }
   return i;
}
